import 'petrie.ecore'

package petrie

context PetriElement
--
-- example invariant with a custom error message to verify that
-- the 'petrinet' property of all 'petrie::PetriElement' instances is non-null
--
inv NonNull_petrinet('The \'petrinet\' property of "' + self.toString() + '" is null'):
	petrinet <> null

endpackage
