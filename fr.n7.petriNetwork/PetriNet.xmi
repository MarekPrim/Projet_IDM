<?xml version="1.0" encoding="UTF-8"?>
<petrie:PetriNet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:petrie="http://petrie"
    xsi:schemaLocation="http://petrie petrie.ecore"
    name="example">
  <petriElements
      xsi:type="petrie:Place"
      name="depart"
      outgoing="//@petriElements.1"
      marking="2"/>
  <petriElements
      xsi:type="petrie:Arc"
      weight="1"
      target="//@petriElements.2"
      source="//@petriElements.0"/>
  <petriElements
      xsi:type="petrie:Transition"
      name="transition"
      incoming="//@petriElements.1"
      outgoing="//@petriElements.3"/>
  <petriElements
      xsi:type="petrie:Arc"
      weight="2"
      target="//@petriElements.4"
      source="//@petriElements.2"/>
  <petriElements
      xsi:type="petrie:Place"
      name="sortie"
      incoming="//@petriElements.3"/>
</petrie:PetriNet>
